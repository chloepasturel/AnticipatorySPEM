    "#axs[0].fill_between(range(N_trials), p[:, 0, 0], lw=.5, alpha=alpha[0][0], facecolor=color[0][0], step='pre')\n",
    "\n",
    "mask = np.ones(N_trials)\n",
    "mask[p[:, num_block, -1]==0] *= np.nan\n",
