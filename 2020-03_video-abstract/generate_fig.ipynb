    "def jolie_fig(ax, background_=True) :\n",
    "    \n",
    "    background = np.zeros((2, 2, 4))\n",
    "    background[:, :, :] = [1, 1, 1, 1]\n",
    "    if type(ax)==np.ndarray :\n",
    "        for a in ax :\n",
    "            a.tick_params(axis='both', colors='w', labelcolor='w')\n",
    "\n",
    "            if background_ :\n",
    "                a.imshow(background, interpolation=\"bicubic\", zorder=0,\n",
    "                          extent=(0, 1, 0, 1), transform=a.transAxes, aspect=\"auto\")\n",
    "            a.xaxis.label.set_color('w')\n",
    "            a.yaxis.label.set_color('w')\n",
    "            \n",
    "            \n",
    "            for card in ['left', 'top', 'bottom', 'right']:\n",
    "                a.spines[card].set_color('w')\n",
    "            \n",
    "            \n",
    "            \n",
    "    else :\n",
    "        \n",
    "        \n",
    "        ax.tick_params(axis='both', colors='w', labelcolor='w')\n",
    "        if background_ :\n",
    "            ax.imshow(background, interpolation=\"bicubic\", zorder=0,\n",
    "                      extent=(0, 1, 0, 1), transform=ax.transAxes, aspect=\"auto\")\n",
    "        ax.xaxis.label.set_color('w')\n",
    "        ax.yaxis.label.set_color('w')\n",
    "        \n",
    "        for card in ['left', 'top', 'bottom', 'right']:\n",
    "            ax.spines[card].set_color('w')\n",
    "        \n",
    "    return ax"
   ]
